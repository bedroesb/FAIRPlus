{
  "id": "https://w3id.org/mircat/minseqe/schema/parameter_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "MINSEQE Parameter schema",
  "description": "The details about a parameter defined by a protocol (e.g. an incubation temperature or a reagent concentration)",
  "type": "object",
  "properties": {
    "@context": {
      "description": "The JSON-LD context",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object"
        },
        {
          "type": "array"
        }
      ]
    },
    "@id": {
      "description": "The JSON-LD identifier",
      "type": "string",
      "format": "uri"
    },
    "@type": {
      "description": "The JSON-LD type",
      "type": "string",
      "format": "uri"
    },
    "name": {
      "description": "a short name for the parameter",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required":["name"]
}



