{
    "id": "https://w3id.org/mircat/miaca_miacme_merge/schema/array_support_size_schema.json",
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "MIACA array support size type schema",
    "description": "Size of reaction vessel, e.g. volume or surface area.",
    "type": "object",
    "properties": {
        "@context": {
            "description": "The JSON-LD context",
            "anyOf": [
                {
                    "type": "string"
                },
                {
                    "type": "object"
                },
                {
                    "type": "array"
                }
            ]
        },
        "@id": {
            "description": "The JSON-LD identifier",
            "type": "string",
            "format": "uri"
        },
        "@type": {
            "description": "The JSON-LD type",
            "type": "string",
            "format": "uri"
        },
        "surfaceArea": {
            "description": "surface area in mm\u00b2",
            "type": "string"
        },
        "dimension": {
            "description": "number of wells",
            "type": "number"
        }
    }
}